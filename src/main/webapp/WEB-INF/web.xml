<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <listener>
        <listener-class>com.hustcaid.myshoppingmanagement.util.FreeMarkerConfiguration</listener-class>
    </listener>
    <servlet>
        <servlet-name>ServletMainPage</servlet-name>
        <servlet-class>com.hustcaid.myshoppingmanagement.webview.MainPage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletMainPage</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

</web-app>